    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "VUE Debug",
            "request": "launch",
            "type": "chrome",
            "url": "https://inertiajs.ddev.site",
            "webRoot": "${workspaceFolder}/src",
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/*"
            }
        },
        {
            "name": "PHP Debug",
            "type": "php",
            "request": "launch",
            "hostname": "inertiajs.ddev.site",
            "port": 9000,
            "pathMappings": {
            "/var/www/html": "${workspaceFolder}"
            }       
        }
    ]
}